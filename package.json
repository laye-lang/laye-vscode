{
	"name": "laye-vscode",
	"displayName": "Laye for VS-Code",
	"description": "Laye support for Visual Studio Code",
	"publisher": "laye-lang",
	"license": "MIT",
	"version": "0.1.8",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/laye-lang/laye-vscode"
	},
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"Laye",
		"laye"
	],
	"contributes": {
		"languages": [
			{
				"id": "laye",
				"aliases": [
					"Laye",
					"laye"
				],
				"extensions": [
					".laye"
				],
				"icon": {
					"dark": "images/dark.png",
					"light": "images/light.png"
				},
				"configuration": "language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "laye",
				"scopeName": "source.laye",
				"path": "syntax/laye.tmLanguage.json"
			}
		]
